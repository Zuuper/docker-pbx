<include>
  <extension name="echo">
    <condition field="destination_number" expression="^9196$">
      <action application="echo"/>
    </condition>
  </extension>
  <extension name="tone">
    <condition field="destination_number" expression="^9198$">
      <action application="playback" data="tone_stream://%(200,200,440)"/>
    </condition>
  </extension>
</include>
