<?xml version="1.0" encoding="UTF-8"?>
<document type="freeswitch/xml">
  <section name="variables">
    <!-- Pick something resolvable by your containers -->
    <X-PRE-PROCESS cmd="set" data="domain=pbx.example.com"/>
    <X-PRE-PROCESS cmd="set" data="local_ip_v4=172.29.16.1"/>
    <X-PRE-PROCESS cmd="set" data="external_sip_ip=auto-nat"/>
    <X-PRE-PROCESS cmd="set" data="external_rtp_ip=auto-nat"/>

    <X-PRE-PROCESS cmd="set" data="internal_sip_port=5060"/>";
    <X-PRE-PROCESS cmd="set" data="external_sip_port=5080"/>"
    <!-- RTP window must match docker published ports -->
    <X-PRE-PROCESS cmd="set" data="rtp_start_port=30000"/>
    <X-PRE-PROCESS cmd="set" data="rtp_end_port=30100"/>

    <!-- ESL password used by FS CLI and FusionPBX installer -->
    <X-PRE-PROCESS cmd="set" data="event_socket_password=12345678"/>

    <!-- Reasonable default codecs -->
    <X-PRE-PROCESS cmd="set" data="global_codec_prefs=opus,PCMU,PCMA,G722"/>
    <X-PRE-PROCESS cmd="set" data="outbound_codec_prefs=opus,PCMU,PCMA,G722"/>

    <!-- Misc -->
    <X-PRE-PROCESS cmd="set" data="default_password=1234"/>
  </section>
</document>
