# ---------- Global ----------
TZ=UTC

# ---------- Postgres (pbx_postgres + used by others) ----------
POSTGRES_DB=postgres
POSTGRES_USER=fusionpbx
# Use a long, random value. Example: openssl rand -base64 24
POSTGRES_PASSWORD=12345678

# ---------- pgAdmin (pbx_pgadmin) ----------
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=12345678

# ---------- FreeSWITCH build toggles (pbx_freeswitch) ----------
# Build from source? true | false   (false = install packages)
FS_SOURCE=false
# For packages: release | master (master requires SWITCH_TOKEN and uses prerelease repo)
FS_BRANCH=release
# If using packages and you want the kitchen sink: true | false
FS_PACKAGE_ALL=false

# Used only if you install prerelease/master packages via fsget.
# Do NOT commit this; keep it in a secrets store.
SWITCH_TOKEN=
